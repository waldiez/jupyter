{
    "name": "@waldiez/jupyter",
    "version": "0.1.2",
    "description": "A Waldiez JupyterLab extension.",
    "keywords": [
        "waldiez",
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://waldiez.github.io/py",
    "bugs": {
        "url": "https://github.com/waldiez/jupyter/issues"
    },
    "license": "MIT",
    "contributors": [
        {
            "name": "Panagiotis Kasnesis",
            "email": "pkasnesis@thingenious.io"
        },
        {
            "name": "Lazaros Toumanidis",
            "email": "laztoum@protonmail.com"
        }
    ],
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,svg,woff2,ttf}",
        "src/**/*.{ts,tsx,svg,css}",
        "schema/*.json"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/waldiez/jupyter.git"
    },
    "scripts": {
        "build": "jlpm build:lib && jlpm build:labextension:dev",
        "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:lib": "tsc --sourceMap",
        "build:lib:prod": "tsc",
        "clean": "jlpm clean:lib",
        "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
        "clean:lintcache": "rimraf .eslintcache .stylelintcache",
        "clean:labextension": "rimraf @waldiez/jupyter/labextension @waldiez/jupyter/_version.py",
        "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
        "eslint": "jlpm eslint:check --fix",
        "eslint:check": "eslint --report-unused-disable-directives --max-warnings 0 --cache src",
        "install:extension": "jlpm build",
        "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
        "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
        "prettier": "jlpm prettier:base --write --list-different",
        "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
        "prettier:check": "jlpm prettier:base --check",
        "stylelint": "jlpm stylelint:check --fix",
        "stylelint:check": "stylelint --cache \"style/**/*.css\"",
        "test": "NODE_NO_WARNINGS=1 jest --coverage",
        "test:ui": "NODE_NO_WARNINGS=1 cd ui-tests && yarn install && yarn test",
        "watch": "run-p watch:src watch:labextension",
        "watch:src": "tsc -w --sourceMap",
        "watch:labextension": "jupyter labextension watch ."
    },
    "dependencies": {
        "@jupyter-widgets/base": "^6.0.10",
        "@jupyter-widgets/jupyterlab-manager": "^5.0.13",
        "@jupyterlab/application": "^4.2.5",
        "@jupyterlab/cells": "^4.2.5",
        "@jupyterlab/codeeditor": "^4.2.5",
        "@jupyterlab/coreutils": "^6.2.5",
        "@jupyterlab/docregistry": "^4.2.5",
        "@jupyterlab/fileeditor": "^4.2.5",
        "@jupyterlab/launcher": "^4.2.5",
        "@jupyterlab/logconsole": "^4.2.5",
        "@jupyterlab/mainmenu": "^4.2.5",
        "@jupyterlab/notebook": "^4.2.5",
        "@jupyterlab/rendermime": "^4.2.5",
        "@jupyterlab/services": "^7.2.5",
        "@jupyterlab/settingregistry": "^4.2.5",
        "@jupyterlab/ui-components": "^4.2.5",
        "@jupyterlab/ui-components-extension": "^4.2.5",
        "@lumino/algorithm": "^2.0.2",
        "@lumino/widgets": "^2.5.0",
        "@waldiez/react": "^0.1.6"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@jupyterlab/builder": "^4.2.5",
        "@jupyterlab/testutils": "^4.2.5",
        "@stylistic/eslint-plugin": "^2.9.0",
        "@testing-library/react": "^16.0.1",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/jest": "^29.5.14",
        "@types/json-schema": "^7.0.15",
        "@types/node": "^22.8.0",
        "@types/react": "^18.3.12",
        "@types/react-addons-linked-state-mixin": "^0.14.27",
        "@typescript-eslint/eslint-plugin": "^8.11.0",
        "@typescript-eslint/parser": "^8.11.0",
        "add": "^2.0.6",
        "copyfiles": "^2.4.1",
        "css-loader": "^7.1.2",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-fetch-mock": "^3.0.3",
        "mini-css-extract-plugin": "^2.9.1",
        "mkdirp": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "stylelint": "^16.10.0",
        "stylelint-config-recommended": "^14.0.1",
        "stylelint-config-standard": "^36.0.1",
        "stylelint-prettier": "^5.0.2",
        "ts-jest": "^29.2.5",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.11.0",
        "yarn-deduplicate": "^6.0.2",
        "yjs": "^13.6.20"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "discovery": {
            "server": {
                "managers": [
                    "pip"
                ],
                "base": {
                    "name": "waldiez_jupyter"
                }
            },
            "kernel": {
                "spec": {
                    "display_name": "waldiez",
                    "language": "^python"
                }
            }
        },
        "schemaDir": "schema",
        "extension": true,
        "outputDir": "waldiez_jupyter/labextension"
    },
    "eslintIgnore": [
        "**/node_modules",
        "**/*.js",
        "**/vs/**",
        "**/.venv",
        "static",
        "dist",
        "coverage",
        "**/*.d.ts",
        "tests",
        "**/__tests__",
        "ui-tests"
    ],
    "prettier": {
        "plugins": [
            "@trivago/prettier-plugin-sort-imports"
        ],
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "endOfLine": "auto",
        "overrides": [
            {
                "files": "package.json",
                "options": {
                    "tabWidth": 4
                }
            }
        ],
        "importOrderSeparation": true,
        "importOrderSortSpecifiers": true,
        "importOrder": [
            "^@jupyterlab/",
            "^@lumino/",
            "^@xyflow/",
            "^react",
            "^[./]"
        ]
    },
    "stylelint": {
        "extends": [
            "stylelint-config-recommended",
            "stylelint-config-standard",
            "stylelint-prettier/recommended"
        ],
        "rules": {
            "property-no-vendor-prefix": null,
            "selector-class-pattern": "^([a-z][A-z\\d]*)(-[A-z\\d]+)*$",
            "selector-no-vendor-prefix": null,
            "value-no-vendor-prefix": null
        }
    },
    "packageManager": "yarn@4.5.1"
}
