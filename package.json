{
    "name": "@waldiez/jupyter",
    "version": "0.7.0",
    "description": "A Waldiez JupyterLab extension.",
    "keywords": [
        "waldiez",
        "ag2",
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://waldiez.io",
    "bugs": {
        "url": "https://github.com/waldiez/jupyter/issues"
    },
    "license": "Apache-2.0",
    "contributors": [
        {
            "name": "Panagiotis Kasnesis",
            "email": "pkasnesis@waldiez.io"
        },
        {
            "name": "Lazaros Toumanidis",
            "email": "laztoum@waldiez.io"
        },
        {
            "name": "Christos Chatzigeorgiou",
            "email": "chrihatz@waldiez.io"
        }
    ],
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,svg,woff2,ttf}",
        "src/**/*.{ts,tsx,svg,css}",
        "schema/*.json"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/waldiez/jupyter.git"
    },
    "scripts": {
        "prepare": "node -e \"(async () => { await import('./.husky/install.mjs').catch(() => {}) })()\"",
        "python": "node --import=tsx scripts/python.ts",
        "requirements": "yarn python -m pip install -r requirements/all.txt",
        "jupyter": "yarn python -m jupyter",
        "before:build": "yarn clean:lib && yarn clean:labextension && yarn lint",
        "build:dev": "yarn build:lib && yarn clean:labextension && yarn build:labextension:dev",
        "build": "yarn before:build && yarn clean:lib && yarn build:lib:prod && yarn build:labextension",
        "build:labextension": "yarn jupyter labextension build .",
        "build:labextension:dev": "yarn jupyter labextension build --development True .",
        "build:lib": "tsc --sourceMap",
        "build:lib:prod": "tsc",
        "clean": "yarn clean:lib && yarn clean:coverage",
        "clean:coverage": "rimraf coverage/js",
        "clean:lib": "rimraf lib tsconfig.tsbuildinfo && rimraf waldiez_jupyter/labextension && rimraf waldiez_jupyter/_version.py",
        "clean:lintcache": "rimraf .eslintcache .stylelintcache",
        "clean:labextension": "rimraf waldiez_jupyter/labextension waldiez_jupyter/_version.py",
        "clean:all": "yarn clean:lib && yarn clean:labextension && yarn clean:lintcache && yarn clean:coverage",
        "eslint": "yarn eslint:check --fix",
        "eslint:check": "eslint --report-unused-disable-directives --no-warn-ignored --max-warnings 0 --cache",
        "install:extension": "yarn build",
        "format": "yarn stylelint && yarn prettier && yarn eslint",
        "lint": "yarn stylelint:check && yarn prettier:check && yarn eslint:check",
        "prettier": "yarn prettier:base --write --list-different",
        "prettier:base": "prettier \"**/*{.ts,.tsx,.css,.json,.md}\"",
        "prettier:check": "yarn prettier:base --check",
        "stylelint": "yarn stylelint:check --fix",
        "stylelint:check": "stylelint --cache \"style/**/*.css\"",
        "test": "NODE_NO_WARNINGS=1 jest --coverage --coverageDirectory=coverage/js --coverageReporters text text-summary lcov && yarn lcov:merge",
        "test:ui": "NODE_NO_WARNINGS=1 cd ui-tests && yarn install && yarn test",
        "lcov:merge": "node --import=tsx scripts/mergeLcov.ts && node --import=tsx scripts/lcovHtml.ts",
        "lcov:html": "lcov-viewer lcov -o coverage/html coverage/lcov.info",
        "version:get": "node --import=tsx scripts/version.ts --get",
        "version:set": "node --import=tsx scripts/version.ts --set",
        "watch": "run-p watch:src watch:labextension",
        "watch:src": "tsc -w --sourceMap",
        "watch:labextension": "yarn jupyter labextension watch ."
    },
    "dependencies": {
        "@jupyter-widgets/base": "^6.0.11",
        "@jupyter-widgets/jupyterlab-manager": "^5.0.15",
        "@jupyterlab/application": "^4.5.1",
        "@jupyterlab/apputils": "^4.6.1",
        "@jupyterlab/cells": "^4.5.1",
        "@jupyterlab/codeeditor": "^4.5.1",
        "@jupyterlab/coreutils": "^6.5.1",
        "@jupyterlab/docregistry": "^4.5.1",
        "@jupyterlab/filebrowser": "^4.5.1",
        "@jupyterlab/fileeditor": "^4.5.1",
        "@jupyterlab/launcher": "^4.5.1",
        "@jupyterlab/logconsole": "^4.5.1",
        "@jupyterlab/mainmenu": "^4.5.1",
        "@jupyterlab/notebook": "^4.5.1",
        "@jupyterlab/rendermime": "^4.5.1",
        "@jupyterlab/services": "^7.5.1",
        "@jupyterlab/settingregistry": "^4.5.1",
        "@jupyterlab/translation": "^4.5.1",
        "@jupyterlab/ui-components": "^4.5.1",
        "@jupyterlab/ui-components-extension": "^4.5.1"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-transform-modules-commonjs": "^7.27.1",
        "@babel/preset-env": "^7.28.5",
        "@babel/preset-react": "^7.28.5",
        "@emotion/is-prop-valid": "^1.4.0",
        "@eslint/js": "^9.39.2",
        "@jest/globals": "^30.2.0",
        "@jupyterlab/builder": "^4.5.1",
        "@jupyterlab/testutils": "^4.5.1",
        "@lcov-viewer/cli": "^1.3.0",
        "@stylistic/eslint-plugin": "^5.6.1",
        "@testing-library/react": "^16.3.1",
        "@trivago/prettier-plugin-sort-imports": "^6.0.0",
        "@types/babel__preset-env": "^7.10.0",
        "@types/jest": "^30.0.0",
        "@types/json-schema": "^7.0.15",
        "@types/node": "^25.0.3",
        "@types/react": "^18.3.27",
        "@types/react-addons-linked-state-mixin": "^0.14.27",
        "@typescript-eslint/eslint-plugin": "^8.50.0",
        "@typescript-eslint/parser": "^8.50.0",
        "@waldiez/react": "0.7.0",
        "all-contributors-cli": "^6.26.1",
        "babel-jest": "^30.2.0",
        "copyfiles": "^2.4.1",
        "css-loader": "^7.1.2",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-headers": "^1.3.3",
        "eslint-plugin-prettier": "^5.5.4",
        "form-data": "^4.0.5",
        "fs-extra": "^11.3.3",
        "husky": "^9.1.7",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^30.2.0",
        "jest-fetch-mock": "^3.0.3",
        "jest-fixed-jsdom": "^0.0.11",
        "lint-staged": "^16.2.7",
        "lookpath": "^1.2.3",
        "mini-css-extract-plugin": "^2.9.4",
        "mkdirp": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.7.4",
        "rimraf": "^6.1.2",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "stylelint": "^16.26.1",
        "stylelint-config-recommended": "^17.0.0",
        "stylelint-config-standard": "^39.0.1",
        "stylelint-prettier": "^5.0.3",
        "tar": "^7.5.2",
        "tmp": "0.2.5",
        "ts-jest": "^29.4.6",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.50.0",
        "yarn-deduplicate": "^6.0.2",
        "yjs": "^13.6.28"
    },
    "resolutions": {
        "form-data": "^4.0.5",
        "tmp": "^0.2.5",
        "tar": "^7.5.2"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,md,yml,yaml}": [
            "yarn run lint"
        ]
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "discovery": {
            "server": {
                "managers": [
                    "pip"
                ],
                "base": {
                    "name": "waldiez_jupyter"
                }
            },
            "kernel": {
                "spec": {
                    "display_name": "waldiez",
                    "language": "^python"
                }
            }
        },
        "schemaDir": "schema",
        "extension": true,
        "outputDir": "waldiez_jupyter/labextension"
    },
    "eslintIgnore": [
        "**/node_modules",
        "**/*.js",
        "**/vs/**",
        "**/.venv",
        "static",
        "dist",
        "coverage",
        "**/*.d.ts",
        "tests",
        "**/__tests__",
        "ui-tests"
    ],
    "prettier": {
        "tabWidth": 4,
        "printWidth": 110,
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "singleQuote": false,
        "trailingComma": "all",
        "endOfLine": "lf",
        "plugins": [
            "@trivago/prettier-plugin-sort-imports"
        ],
        "importOrderSeparation": true,
        "importOrderSortSpecifiers": true,
        "importOrder": [
            "^react",
            "^react-dom",
            "@waldiez/react",
            "^@waldiez/jupyter"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.yml",
                    "**/*.yaml",
                    "**/*.md",
                    "**/*.css"
                ],
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "stylelint": {
        "extends": [
            "stylelint-config-recommended",
            "stylelint-config-standard",
            "stylelint-prettier/recommended"
        ],
        "rules": {
            "property-no-vendor-prefix": null,
            "selector-class-pattern": "^([a-z][A-z\\d]*)(-[A-z\\d]+)*$",
            "selector-no-vendor-prefix": null,
            "value-no-vendor-prefix": null
        }
    },
    "packageManager": "yarn@4.12.0"
}
